-	// See above.
-	struct dispatch_magazine_header_s header;
-
-	// Align supermaps as needed.
-#if HEADER_TO_SUPERMAPS_PADDING > 0
-	char _pad0[HEADER_TO_SUPERMAPS_PADDING];
-#endif
-
-	// Second-level bitmap; each set bit means a bitmap_t in maps[][]
