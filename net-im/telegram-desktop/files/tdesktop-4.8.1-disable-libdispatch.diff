-static void
-_dispatch_muxnote_dispose(dispatch_muxnote_t dmn)
-{
-	if (dmn->dmn_thread || dmn->dmn_threadpool_wait) {
-		DISPATCH_INTERNAL_CRASH(0, "disposed a muxnote with an active thread");
-	}
-	if (dmn->dmn_event) {
-		CloseHandle(dmn->dmn_event);
-	}
-	free(dmn);
-}
