-#if PACK_FIRST_PAGE_WITH_CONTINUATIONS
-	// (the base of c's magazine == the base of c's page)
-	// => c is in first page of magazine
-	return (((uintptr_t)c & MAGAZINE_MASK) ==
-			((uintptr_t)c & ~(uintptr_t)DISPATCH_ALLOCATOR_PAGE_MASK));
-#else
-	(void)c;
