-	uintptr_t refcount = os_atomic_dec(&dmn->dmn_refcount, relaxed);
-	if (refcount == 0) {
-		_dispatch_muxnote_dispose(dmn);
-	} else if (refcount == UINTPTR_MAX) {
-		DISPATCH_INTERNAL_CRASH(0, "muxnote refcount underflow");
-	}
-}
