-			// Continuous time (value 10 in bits 63, 62).
-			*clock = DISPATCH_CLOCK_MONOTONIC;
-			actual_value = time & ~DISPATCH_UP_OR_MONOTONIC_TIME_MASK;
-		}
-	} else {
-		*clock = DISPATCH_CLOCK_UPTIME;
-		actual_value = time;
