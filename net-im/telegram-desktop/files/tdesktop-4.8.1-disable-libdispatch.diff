-static void
-firehose_snapshot_tickle_clients(firehose_snapshot_t fs, bool for_io)
-{
-	firehose_client_t fc;
-	uint32_t n = 0;
-
-	fs_clients_lock();
-	TAILQ_FOREACH(fc, &server_config.fs_clients, fc_entry) {
-		if (unlikely(fc->fc_memory_corrupted)) {
-			continue;
-		}
-		if (!fc->fc_pid) {
-#if TARGET_OS_SIMULATOR
-			continue;
-#endif
