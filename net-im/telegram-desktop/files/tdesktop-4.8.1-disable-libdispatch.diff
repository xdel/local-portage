-			}
-#else
-			processed = write(op->fd_entry->fd, buf, len);
-#endif
-		} else if (op->params.type == DISPATCH_IO_RANDOM) {
-#if defined(_WIN32)
-			OVERLAPPED ovlOverlapped = {};
-			ovlOverlapped.Offset = off & 0xffffffff;
