-		if (dr->du_timer_flags & DISPATCH_TIMER_AFTER) {
-			_dispatch_timer_unote_disarm(dr, dth); // +2 is consumed by _merge_evt()
-			_dispatch_wlh_release(_dispatch_unote_wlh(dr));
-			_dispatch_unote_state_set(dr, DU_STATE_UNREGISTERED);
