-		if (unlikely(old_value & DLOCK_FAILED_TRYLOCK_BIT)) {
-			new_value = old_value ^ DLOCK_FAILED_TRYLOCK_BIT;
-		} else {
-			new_value = DLOCK_OWNER_NULL;
-		}
