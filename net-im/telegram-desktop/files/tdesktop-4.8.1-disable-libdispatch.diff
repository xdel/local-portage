-			// and doesn't rearm the return-to-kernel notification
-			// We need to assume we have to go back.
-			_dispatch_set_return_to_kernel();
-		}
-#endif // DISPATCH_USE_KEVENT_WORKLOOP
-		for (i = 0, r = 0; i < n; i++) {
-			_dispatch_kevent_drain(&ke_out[i]);
-		}
-	}
-	return r;
-}
