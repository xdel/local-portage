-#define DISPATCH_APPLY_MAX UINT16_MAX // must be < sqrt(SIZE_MAX)
-
-DISPATCH_ALWAYS_INLINE
-static inline dispatch_queue_global_t
-_dispatch_apply_root_queue(dispatch_queue_t dq)
-{
-	if (dq) {
-		while (unlikely(dq->do_targetq)) {
-			dq = dq->do_targetq;
-		}
