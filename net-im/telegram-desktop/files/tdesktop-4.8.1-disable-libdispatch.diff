- */
-API_AVAILABLE(macos(10.7), ios(5.0))
-DISPATCH_EXPORT DISPATCH_NONNULL1 DISPATCH_NONNULL3 DISPATCH_RETURNS_RETAINED
-DISPATCH_WARN_RESULT DISPATCH_NOTHROW
-dispatch_data_t
-dispatch_data_copy_region(dispatch_data_t data,
